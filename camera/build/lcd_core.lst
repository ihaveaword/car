  66:BSP/LCD_ST7789/lcd_core.c ****     /* 行地址设置 */
  67:BSP/LCD_ST7789/lcd_core.c ****     lcd_config_reg(plcd->io, 0x2b);
  68:BSP/LCD_ST7789/lcd_core.c ****     lcd_write_halfword(plcd->io, y1 + lcd_cfg_address[plcd->hw->type][plcd->hw->rotate][2]);
  69:BSP/LCD_ST7789/lcd_core.c ****     lcd_write_halfword(plcd->io, y2 + lcd_cfg_address[plcd->hw->type][plcd->hw->rotate][3]);
  70:BSP/LCD_ST7789/lcd_core.c ****     /* 储存器写 */
  71:BSP/LCD_ST7789/lcd_core.c ****     lcd_config_reg(plcd->io, 0x2c);
  72:BSP/LCD_ST7789/lcd_core.c **** }
