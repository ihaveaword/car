  60:BSP/LCD_ST7789/lcd_core.c **** void lcd_set_address(lcd* plcd, uint16_t x1, uint16_t y1, uint16_t x2, uint16_t y2)
  61:BSP/LCD_ST7789/lcd_core.c **** {
  62:BSP/LCD_ST7789/lcd_core.c ****     /* 列地址设置 */
  63:BSP/LCD_ST7789/lcd_core.c ****     lcd_config_reg(plcd->io, 0x2a);
  64:BSP/LCD_ST7789/lcd_core.c ****     lcd_write_halfword(plcd->io, x1 + lcd_cfg_address[plcd->hw->type][plcd->hw->rotate][0]);
